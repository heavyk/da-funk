  "aLIVEsCRIPT": {
    "directories": {
      "src": {},
      "lib": {}
    },
    "compile": {
      "package.json": "./src/package.json.ls"
    }
  },
  "da-funk": {
    "doT-funk.description": "make programming as fun as listening to bumpin basslines",
    "doT-funk.README": "# doT-funk\nTODO: write me!\nlolz, getting there eventually...",
    "doT-funk": {
      "format": "",
      "evaluate": "/\\{\\{([\\s\\S]+?)\\}\\}/",
      "interpolate": "/\\{\\{=([\\s\\S]+?)\\}\\}/",
      "encode": "/\\{\\{!([\\s\\S]+?)\\}\\}/",
      "use": "/\\{\\{#([\\s\\S]+?)\\}\\}/",
      "define": "/\\{\\{##\\s*([\\w\\.$]+)\\s*(\\:|=)([\\s\\S]+?)#\\}\\}/",
      "conditional": "/\\{\\{\\?(\\?)?\\s*([\\s\\S]*?)\\s*\\}\\}/",
      "iterate": "/\\{\\{~\\s*(?:\\}\\}|([\\s\\S]+?)\\s*\\:\\s*([\\w$]+)\\s*(?:\\:\\s*([\\w$]+))?\\s*\\}\\})/",
      "varname": "boogie",
      "strip": false,
      "append": true,
      "selfcontained": false
    },
    "meta.description": "this is for future use with forkdb",
    "meta": {
      "_key": "heavyk/da-funk",
      "_origin": []
    },
    "compilation": {
      "file:package.json": [
        "version"
      ],
      "file:lib/da_funk.js": [
        ".stringify",
        ".freedom",
        ".interject",
        ".objectify",
        ".merge",
        ".extend",
        ".improve",
        ".embody"
      ]
    }
  }
}
